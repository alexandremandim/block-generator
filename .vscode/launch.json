    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) gerador",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/gerador",
            "args": [ "-r", "./input.txt", "-w", "/home/alexandre/Desktop/gerado/debug/teste", "-n", "10000000", "-a", "5", "-p", "50"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}